<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.future.association.personal.ui.activity.AboutUsActivity">

    <include layout="@layout/layout_toolbar" />

    <ImageView
        android:id="@+id/imgXiaoXie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_hhhbig_60"
        android:src="@drawable/ic_dengji" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imgXiaoXie"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/margin_lsmall_10"
